{
    "name": "queue_backend",
    "version": "0.0.1",
    "description": "REST API for queue management app",
    "author": "",
    "main": "server.js",
    "private": true,
    "license": "ISC",
    "engines": {
        "node": ">= 14.16 <15"
    },
    "scripts": {
        "start": "node build/server.js",
        "dev": "concurrently \"npm run build:watch\" \"npm start\"",
        "build": "npm run lint && tsc -p tsconfig.release.json",
        "build:watch": "tsc -w -p tsconfig.release.json",
        "lint": "eslint . --ext .ts,.js && tsc -p tsconfig.json",
        "prettify": "prettier --write .",
        "up": "migrate-mongo up",
        "down": "migrate-mongo down",
        "status": "migrate-mongo status",
        "create": "migrate-mongo create",
        "prepare": "husky install"
    },
    "dependencies": {
        "ajv": "^8.6.0",
        "ajv-errors": "^3.0.0",
        "ajv-formats": "^2.1.0",
        "bcrypt": "^5.0.1",
        "bcryptjs": "^2.4.3",
        "bluebird": "^3.7.2",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "errorhandler": "^1.5.1",
        "express": "^4.17.1",
        "formidable": "^1.2.2",
        "helmet": "^4.6.0",
        "http-errors": "^1.8.0",
        "jsonwebtoken": "^8.5.1",
        "migrate-mongo": "^8.2.2",
        "mongoose": "^5.12.14",
        "mongoose-beautiful-unique-validation": "^7.1.1",
        "morgan": "^1.10.0",
        "path": "^0.12.7",
        "read-chunk": "^3.2.0",
        "swagger-ui-express": "^4.1.6",
        "tslib": "~2.3.0",
        "uuid": "3.3.2"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/bluebird": "^3.5.35",
        "@types/cors": "^2.8.10",
        "@types/express": "^4.17.12",
        "@types/formidable": "^1.2.2",
        "@types/fs-extra": "^9.0.12",
        "@types/http-errors": "^1.8.0",
        "@types/jsonwebtoken": "^8.5.2",
        "@types/morgan": "^1.9.3",
        "@types/node": "~14.14.45",
        "@types/swagger-ui-express": "^4.1.2",
        "@types/uuid": "^8.3.1",
        "@typescript-eslint/eslint-plugin": "~4.27.0",
        "@typescript-eslint/parser": "~4.27.0",
        "concurrently": "^6.2.0",
        "eslint": "~7.28.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "husky": "^7.0.0",
        "prettier": "~2.3.1",
        "pretty-quick": "^3.1.1",
        "shelljs": "^0.8.4",
        "tsutils": "~3.21.0",
        "typescript": "~4.3.2"
    }
}
